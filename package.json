{
  "name": "scdb-eventviewer",
  "version": "2.0.0",
  "description": "A self made event viewer to render events in game",
  "author": "ShinyColorsDB",
  "license": "MIT",
  "keywords": [
    "pixiJS",
    "idolmaster-shinycolors",
    "idolmaster",
    "shinycolors"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/scdb-eventviewer.js",
  "module": "./dist/scdb-eventviewer.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/scdb-eventviewer.es.js",
      "require": "./dist/scdb-eventviewer.min.js"
    }
  },
  "scripts": {
    "dev": "vite --port 5500",
    "build": "rimraf dist && node scripts/build.js",
    "build:vite": "rimraf dist && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@pixi/sound": "^6.0.0",
    "@types/node": "^20.3.2",
    "gsap": "^3.12.1",
    "pixi.js": "^8.0.4",
    "rimraf": "^5.0.1",
    "terser": "^5.18.2",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-dts": "^3.0.0-beta.3"
  },
  "peerDependencies": {
    "pixi.js": "^8.0.0"
  },
  "dependencies": {
    "csvjson-csv2json": "^5.0.6"
  }
}
